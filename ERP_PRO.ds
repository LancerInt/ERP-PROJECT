										// --- Build before & after summaries ---
										before_data = "---- BEFORE EDIT ----\n";
										after_data = "---- AFTER EDIT ----\n";
										// --- Add the fields you want to compare ---
										fields_to_track = {"Company_Name","GST_Number","Email","Address"};
										for each  f in fields_to_track
										{
											old_value = ifnull(old_record.get(f),"");
											new_value = input.legal_name;
											before_data = before_data + f + " : " + old_value + "\n";
											after_data = after_data + f + " : " + new_value + "\n";
										}
										// --- Combine content ---
										mail_content = "A record has been UPDATED by " + zoho.loginuser + " on " + zoho.currenttime + "\n\n" + before_data + "\n" + after_data;
