			GST_Validation as "GST Validation"
			{
				type =  form
				form = company
				record event = on add

				on user input of gstin
				{
					actions 
					{
						custom deluge script
						(
											gstin_value = ifnull(input.gstin,"").trim().toUpperCase();
										gstin_pattern = "^[0-9]{2}[A-Z]{5}[0-9]{4}[A-Z]{1}[1-9A-Z]{1}[0-9A-Z]{2}$";
										if(!gstin_value.isEmpty())
										{
											if(gstin_value.length() != 15)
											{
												alert "GST Number must be exactly 15 characters long.";
												input.gstin = "";
											}
											else if(!gstin_value.matches(gstin_pattern))
											{
												alert "Invalid GST Number format. Example: 27ABCDE1234F2Z5 or 07ABCDE1234A9X2.";
												input.gstin = "";
											}
											else
											{
												info " GST Number format is valid: " + gstin_value;
												GSTIN = gstin_value;
												// normalize to uppercase
											}
										}
						)
					}
				}

