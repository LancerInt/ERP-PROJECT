			{
				type =  form
				form = Tax_Master
				record event = on add

				on user input of tax_type
				{
					actions 
					{
						custom deluge script
						(
											if(input.tax_type != null)
										{
											show tax_number;
											if(input.tax_type == "GST")
											{
												input.tax_number = "GST Number :";
											}
											else if(input.tax_type == "TDS")
											{
												input.tax_number = "TDS Registration Number :";
											}
											else
											{
												input.tax_number = "Tax Reference Number :";
											}
										}
										else
