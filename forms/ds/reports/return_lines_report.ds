list return_lines_report
{
	displayName = "Return Lines Report"
	show all rows from return_lines
	(
		batch as "Batch"
		quantity_returned as "Quantity Returned"
		uom as "UOM"
		condition as "Condition"
		viability_notes as "Viability Notes"
	)
	quickview
	(
		layout
		(
			datablock1
			(
				title = "Return Lines Overview"
				fields
				(
					batch as "Batch"
					quantity_returned as "Quantity Returned"
					uom as "UOM"
					condition as "Condition"
					viability_notes as "Viability Notes"
				)
			)
		)
		menu
		(
			header
			(
				Edit
				Duplicate
				Delete
			)
			record
			(
				Edit
				Duplicate
				Delete
			)
		)
		action
		(
			on click
			(
				View Record
			)
			on right click
			(
				Edit
				Delete
				Duplicate
				View Record
			)
		)
	)
	detailview
	(
		layout
		(
			datablock1
			(
				title = "Return Lines Overview"
				fields
				(
					batch as "Batch"
					quantity_returned as "Quantity Returned"
					uom as "UOM"
					condition as "Condition"
					viability_notes as "Viability Notes"
				)
			)
		)
		menu
		(
			header
			(
				Edit
				Duplicate
				Delete
			)
		)
	)
}
