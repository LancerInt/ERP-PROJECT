list quote_evaluation_report
{
	displayName = "Quote Evaluation Report"
	show all rows from quote_evaluation
	(
		evaluation_id as "Evaluation ID"
		evaluation_date as "Evaluation Date"
		comparison_table as "Comparison Table"
		best_quote_flag as "Best Quote Flag"
		justification_notes as "Justification Notes"
		approval_status as "Approval Status"
		approval_trail as "Approval Trail"
	)
	quickview
	(
		layout
		(
			datablock1
			(
				title = "Quote Evaluation Overview"
				fields
				(
					evaluation_id as "Evaluation ID"
					evaluation_date as "Evaluation Date"
					comparison_table as "Comparison Table"
					best_quote_flag as "Best Quote Flag"
					justification_notes as "Justification Notes"
					approval_status as "Approval Status"
					approval_trail as "Approval Trail"
				)
			)
		)
		menu
		(
			header
			(
				Edit
				Duplicate
				Delete
			)
			record
			(
				Edit
				Duplicate
				Delete
			)
		)
		action
		(
			on click
			(
				View Record
			)
			on right click
			(
				Edit
				Delete
				Duplicate
				View Record
			)
		)
	)
	detailview
	(
		layout
		(
			datablock1
			(
				title = "Quote Evaluation Overview"
				fields
				(
					evaluation_id as "Evaluation ID"
					evaluation_date as "Evaluation Date"
					comparison_table as "Comparison Table"
					best_quote_flag as "Best Quote Flag"
					justification_notes as "Justification Notes"
					approval_status as "Approval Status"
					approval_trail as "Approval Trail"
				)
			)
		)
		menu
		(
			header
			(
				Edit
				Duplicate
				Delete
			)
		)
	)
}
